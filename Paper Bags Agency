import java.util.*;
public class Paper
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int w=sc.nextInt();
        int x=sc.nextInt();
        int y=sc.nextInt();
        int a=w*x;
        int b=w*y;
        int c=a-b;
        int d=c-100;
        System.out.println(d);
    }
}
