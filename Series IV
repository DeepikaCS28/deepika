import java.util.*;
public class Series4
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        double b=0.5;
        double c=b;
        System.out.println(b);
        for(int i=1;i<=a-1;i++)
        {
            c=c*3;
            System.out.println(c);
        }
    }
}
