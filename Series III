import java.util.*;
public class Series3
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=1;
        int n=3;
        for(int i=1;i<=a;i++)
        {
            b=b*n;
            System.out.println(b);
        }
    }
}
