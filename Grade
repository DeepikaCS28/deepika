import java.util.Scanner;
public class Garden
{
    public static void main(String args[])
    {
       Scanner sc=new Scanner(System.in);
       int a=sc.nextInt();
       int b=sc.nextInt();
       int i=sc.nextInt();
       if(i<=1 && i>=b || i%b==0 || i%b==1)
       {
           System.out.println("Yes");
       }
       else 
       {
           System.out.println("no");
       }
    }
}
